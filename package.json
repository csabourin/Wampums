{
  "name": "wampums",
  "version": "3.1.0",
  "description": "Wampums project",
  "main": "api.js",
  "scripts": {
    "dev": "vite",
    "dev:all": "npm run start & npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "ANALYZE=true vite build",
    "start": "node api.js",
    "start:prod": "NODE_ENV=production node api.js",
    "postinstall": "npm rebuild bcrypt --build-from-source",
    "test": "jest",
    "lint:api-version": "node scripts/modernization/check-api-version.js",
    "lint:duplicate-mounts": "node scripts/modernization/check-duplicate-mounts.js",
    "lint:spa-files": "node scripts/modernization/check-spa-files.js",
    "lint:spa-console": "bash -c \"rg \\\"console\\\\.\\\" spa --glob \\\"!spa/utils/DebugUtils.js\\\" --glob \\\"!spa/config.js\\\" && echo \\\"Console usage found in SPA\\\" && exit 1 || exit 0\"",
    "lint:spa-innerhtml": "bash -c \"rg \\\"innerHTML\\\\s*=\\\" spa --glob \\\"!spa/utils/DOMUtils.js\\\" --glob \\\"!spa/utils/SecurityUtils.js\\\" && echo \\\"innerHTML assignment found in SPA\\\" && exit 1 || exit 0\"",
    "lint:sql-params": "node scripts/modernization/check-sql-params.js",
    "lint:non-versioned-mounts": "node scripts/modernization/check-non-versioned-mounts.js",
    "test:quality": "jest --runInBand --testPathIgnorePatterns=\"/node_modules/|/mobile/|test/api.test.js|test/api-consistency.test.js|test/security.test.js|test/activities.create.test.js|test/push-subscription.test.js|test/permission-slips.security.test.js|test/landing-host.test.js|test/local-groups.test.js|test/resources.equipment.test.js|tests/manual_budget_test.js\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.2.0",
    "@googleapis/chat": "^44.5.0",
    "@supabase/supabase-js": "^2.93.3",
    "@veryfi/veryfi-sdk": "^1.4.6",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "bcryptjs": "^3.0.3",
    "body-parser": "^2.2.2",
    "compression": "^1.8.0",
    "cors": "^2.8.6",
    "dexie": "^4.3.0",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "glob": "^13.0.3",
    "google-auth-library": "^10.5.0",
    "heic-convert": "^2.1.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mindee": "^4.36.3",
    "multer": "^2.0.2",
    "nodemailer": "^8.0.1",
    "openai": "^6.16.0",
    "pg": "^8.18.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.1",
    "sib-api-v3-sdk": "^8.5.0",
    "socket.io": "^4.8.1",
    "stripe": "^20.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "web-push": "^3.6.7",
    "winston": "^3.17.0",
    "workbox-cacheable-response": "^7.4.0",
    "workbox-expiration": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-strategies": "^7.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "fake-indexeddb": "^6.2.5",
    "jest": "^30.2.0",
    "supertest": "^7.2.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setupJest.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mobile/"
    ],
    "transform": {
      "^.+/(spa|tests|test)/.+\\.js$": [
        "babel-jest",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "targets": {
                  "node": "current"
                }
              }
            ]
          ]
        }
      ]
    }
  }
}
